{
  "version": 2,
  "builds": [
    {
      "src": "dist/happy-ghumakkads/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/happy-ghumakkads/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/happy-ghumakkads/server/server.mjs"
    }
  ],
  "functions": {
    "dist/happy-ghumakkads/server/server.mjs": {
      "runtime": "nodejs20.x"
    }
  }
}